{
  "name": "edubdd",
  "version": "1.0.0",
  "description": "automation test with cypress bdd",
  "main": "index.js",
  "repository": "https://github.com/Albarokah9/edubdd.git",
  "author": "albar",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^14.4.1",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "reflect-metadata": "^0.2.2"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:check:all": "prettier --check .",
    "prettier:fix:all": "prettier --write .",
    "prettier:check:json": "prettier --check ./**/*.json",
    "prettier:fix:json": "prettier --write ./**/*.json",
    "prettier:check:js": "prettier --check ./**/*.js",
    "prettier:fix:js": "prettier --write ./**/*.js",
    "prettier:check:ts": "prettier --check ./**/*.ts",
    "prettier:fix:ts": "prettier --write ./**/*.ts",
    "prettier:check:html": "prettier --check ./**/*.html",
    "prettier:fix:html": "prettier --write ./**/*.html",
    "prettier:check:css": "prettier --check ./**/*.css",
    "prettier:fix:css": "prettier --write ./**/*.css",
    "prettier:check:md": "prettier --check ./**/*.md",
    "prettier:fix:md": "prettier --write ./**/*.md",
    "prettier:check:yaml": "prettier --check ./**/*.yaml",
    "prettier:fix:yaml": "prettier --write ./**/*.yaml",
    "prettier:check:scss": "prettier --check ./**/*.scss",
    "prettier:fix:scss": "prettier --write ./**/*.scss",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:electron": "cypress run --browser electron",
    "cy:open:chrome": "cypress open --browser chrome",
    "cy:open:firefox": "cypress open --browser firefox",
    "cy:open:edge": "cypress open --browser edge",
    "cy:open:electron": "cypress open --browser electron"
  }
}
